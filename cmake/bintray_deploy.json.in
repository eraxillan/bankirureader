{
    "package": {
        "name": "Rubankireader-git",
        "repo": "Rubankireader",
        "subject": "rubankireader",
        "desc": "Rubankireader master branch automated builds",
        "website_url": "https://github.com/eraxillan/bankirureader",
        "issue_tracker_url": "https://github.com/eraxillan/bankirureader/issues",
        "vcs_url": "https://github.com/eraxillan/bankirureader.git",
        "github_use_tag_release_notes": true,
        "github_release_notes_file": "RELEASE.txt",
        "licenses": ["GPL-2.0"],
        "labels": ["card", "tabletop", "game"],
        "public_download_numbers": false,
        "public_stats": true
    },
    "version": {
        "name": "@PROJECT_VERSION@",
        "desc": "Unstable builds from master",
        "vcs_tag": "@GIT_COMMIT_ID@",
        "gpgSign": false
    },
    "files": [
        { "includePattern": "build/(Rubankireader.*\\.deb)", "uploadPattern": "Ubuntu/$ENV{DIST}/$1" },
        { "includePattern": "build/(Rubankireader.*\\.dmg)", "uploadPattern": "MacOSX/$1" }
    ],
    "publish": true
}
